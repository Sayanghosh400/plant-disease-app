{
  "expo": {
    "name": "plant-disease-app",
    "slug": "plant-disease-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/planticon.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/cactus.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/planticon.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sayanghosh10.plantdiseaseapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/planticon.jpg"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0e294ce0-7295-46cc-aa39-1182b9b33f18"
      }
    }
  }
}
